#header
  width 680px
  margin 50px auto  
  padding 30px 60px
  overflow auto !important
  border-radius 5px
  background #f5f5f5
  .avatar  
    height 80px
    width 80px
    border-radius 40px
  a
    color #999
    &:hover
      color color-link
      text-decoration none 
  
  #main-nav
