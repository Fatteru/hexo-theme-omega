<a href="/" class="alignleft">
  <img class="avatar" src="<%- gravatar(config.email, 80) %>" title="<%= config.title %>"/>
</a>
<nav id="main-nav" class="alignleft clearfix">
  <ul>
    <% for (var i in theme.menu){ %>
      <li><a href="<%- theme.menu[i] %>"><%= i %></a></li>
    <% } %>
  </ul>
</nav>